{
  "name": "samay",
  "version": "0.2.0",
  "description": "Samay is an elegant wrapper around JavaScript Date with custom functions.",
  "keywords":["Date", "JavaScript Date Wrapper"],
  "main": "Samay.js",
  "scripts": {
    "build":"babel Samay.js --out-dir lib",
    "test": "nyc ava --verbose",
    "posttest":"nyc report --reporter=html"
  },
  "keywords": [
    "JavaScript",
    "Date"
  ],
  "author": "Neerav Pandya",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-env": "^2.0.24",
    "nyc": "^10.1.2"
  }
}
